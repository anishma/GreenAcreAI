# =============================================================================
# GreenAcre AI - Environment Variables Template
# =============================================================================
# Copy this file to .env.local and fill in your actual values
# =============================================================================

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=sb_publishable_xxx
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJxxx (legacy, use publishable key instead)
SUPABASE_SECRET_KEY=sb_secret_xxx (preferred for server-side)
SUPABASE_SERVICE_ROLE_KEY=eyJxxx (legacy, use secret key instead)
SUPABASE_DB_PASSWORD=your-db-password
DATABASE_URL=postgresql://postgres:[PASSWORD]@db.your-project.supabase.co:5432/postgres

# Next.js
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Vercel
VERCEL_PROJECT_ID=
VERCEL_ORG_ID=
VERCEL_TOKEN=

# OpenAI
OPENAI_API_KEY=sk-xxx
OPENAI_ORG_ID=

# VAPI
VAPI_API_KEY=
VAPI_WEBHOOK_SECRET=
VAPI_PHONE_NUMBER_ID=

# Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_xxx
STRIPE_SECRET_KEY=sk_test_xxx
STRIPE_WEBHOOK_SECRET=whsec_xxx
STRIPE_PRICE_ID_STARTER=price_xxx
STRIPE_PRICE_ID_PROFESSIONAL=price_xxx
STRIPE_PRICE_ID_ENTERPRISE=price_xxx

# Regrid API
REGRID_API_KEY=

# Google Cloud
GOOGLE_CLIENT_ID=xxx.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:3000/api/auth/callback/google

# Sentry
NEXT_PUBLIC_SENTRY_DSN=https://xxx@sentry.io/xxx
SENTRY_AUTH_TOKEN=
SENTRY_ORG=
SENTRY_PROJECT=

# Secrets (generate with: openssl rand -base64 32)
NEXTAUTH_SECRET=
JWT_SECRET=
ENCRYPTION_KEY=

# Development
NODE_ENV=development
SKIP_ENV_VALIDATION=false
